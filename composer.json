{
    "name": "owenbush/static-settings",
    "description": "An API for storing and retrieving static settings",
    "type": "library",
    "authors": [
        {
            "name": "Owen Bush",
            "email": "owen@obush.co.uk"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "StaticSettings\\": "src/",
            "StaticSettings\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.11",
        "drupal/coder": "^8.3"
    },
    "license": "GPL-2.0-or-later",
    "minimum-stability": "stable",
    "autoload-dev": {
        "psr-4": {
            "StaticSettings\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
